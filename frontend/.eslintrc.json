/* eslint-disable react/prop-types */
{
  "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "airbnb",
      "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
      "no-restricted-syntax": ["error", "ForInStatement", "LabeledStatement", "WithStatement"],
      "no-plusplus": 0,
      "react/jsx-filename-extension": 0,
      "react/no-array-index-key" : 0,
      "react/prop-types": "off",
      "no-bitwise": 0
    }
}

