/* eslint-disable react/prop-types */
{
  "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "airbnb",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
    ],
    "rules": {
      "no-restricted-syntax": ["error", "ForInStatement", "LabeledStatement", "WithStatement"],
      "no-plusplus": 0,
      "react/prop-types": "off"
    }
}
